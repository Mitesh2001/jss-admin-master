{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "fideloper/proxy": "^4.2",
        "freshbitsweb/laratables": "^2.3",
        "google/recaptcha": "^1.2.3",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.2",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/tinker": "^2.0",
        "maatwebsite/excel": "^3.1",
        "php-http/guzzle6-adapter": "^2.0",
        "pragmarx/google2fa-laravel": "^1.1",
        "sparkpost/sparkpost": "^2.2",
        "spatie/browsershot": "^3.35",
        "spatie/laravel-activitylog": "^3.13",
        "stripe/stripe-php": "^7.27",
        "fruitcake/laravel-cors": "^1.0",
        "laravel/ui": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-query-detector": "^1.2",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "laravel/dusk": "^5.10",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan clear-compiled",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --nowrite",
            "@php -r \"is_dir('storage/app/downloads/') || mkdir('storage/app/downloads/');\"",
            "@php -r \"is_dir('storage/app/downloads/receipts') || mkdir('storage/app/downloads/receipts');\"",
            "@php -r \"is_dir('storage/app/downloads/front-attendance') || mkdir('storage/app/downloads/front-attendance');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
